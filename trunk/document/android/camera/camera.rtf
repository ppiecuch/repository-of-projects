\clcbpat7\clbrdrl\brdrw10\brdrs\brdrcf5\clbrdrt\brdrw10\brdrs\brdrcf5\clbrdrr\brdrw10\brdrs\brdrcf5\clbrdrb\brdrw10\brdrs\brdrcf5 \cellx8414\pard\intbl\fi426 public static void setCameraDisplayOrientation ( Activity activity , \par
          int cameraId , android.hardware.Camera camera ) \{ \par
     android.hardware.Camera.CameraInfo info = \par
              new android.hardware.Camera.CameraInfo (); \par
     android.hardware.Camera.getCameraInfo ( cameraId , info ); \par
      int rotation = activity.getWindowManager (). getDefaultDisplay () \par
             .getRotation (); \par
      int degrees = 0 ; \par
      switch ( rotation ) \{ \par
          case Surface.ROTATION_0 : degrees = 0 ; break ; \par
          case Surface.ROTATION_90 : degrees = 90 ; break ; \par
          case Surface.ROTATION_180 : degrees = 180 ; break ; \par
          case Surface.ROTATION_270 : degrees = 270 ; break ; \par
      \} \par
\par
      int result ; \par
      if ( info.facing == Camera.CameraInfo.CAMERA_FACING_FRONT ) \{ \par
         result = ( info.orientation + degrees ) % 360 ; \par
         result = ( 360 - result ) % 360 ;   // compensate the mirror \par
      \} else \{   // back-facing \par
         result = ( info.orientation - degrees + 360 ) % 360 ; \par
      \} \par
     camera.setDisplayOrientation ( result ); \par
  \} \par
