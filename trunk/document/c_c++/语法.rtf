int main ()\par
\{\par
  char str[] ="- This, a sample string.";\par
  char * pch;\par
  printf ("Splitting string \\"%s\\" into tokens:\\n",str);\par
  pch = strtok (str," ,.-");\par
  while (pch != NULL)\par
  \{\par
    printf ("%s\\n",pch);\par
    pch = strtok (NULL, " ,.-");\par
  \}\par
  return 0;\par
\}\par
